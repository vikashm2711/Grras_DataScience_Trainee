      "source": [
        "\n",
        "from random import randint\n",
        "values=[]\n",
        "for i in range(100000):\n",
        "    x=randint(0,101)\n",
        "    values.append(x)\n",
        "# print(values)\n",
        "fd={}\n",
        "for x in values:\n",
        "    if x in fd:\n",
        "        fd[x]=x+1\n",
        "    else:\n",
        "        fd[x]=1\n",
        "for q,w in fd.items():\n",
        "    print(f\"value= {q}      |    frequency= {w} times\")"
      ],
